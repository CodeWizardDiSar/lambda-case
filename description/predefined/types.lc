
or_type EmptyVal
values
  empty_val

or_type Bool
values
  true | false

or_type Possibly(T)
values
  the_value:T | no_value

or_type ListOf(T)s
values
  non_empty:HeadAndTailOf(T)s | empty

tuple_type HeadAndTailOf(T)s
value
  (head, tail): T x ListOf(T)s

or_type Reslut(R)OrError(E)
values
  result:R | error:E

type_nickname IO = (EmptyVal)FromIO
