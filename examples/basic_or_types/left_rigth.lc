a, b: Int-->Or<--String, Int-->Or<--String
  = LeftType 1, RightType "One"

print_int_or_string: Int-->Or<--String -> IO Empty
  = case 
    LeftType -> get-->( + 1 )-->to_string-->( "Total after adding 1: " + )-->print
    RightType -> get-->( "Turns out it says: " + )-->print

main: IO Empty
  = a-->print_int_or_string ;> b-->print_int_or_string
