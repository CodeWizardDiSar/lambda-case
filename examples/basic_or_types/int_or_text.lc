
a, b: all Int==>Or<==Text
  = from_left<==1, from_right<=="Hi!"

print_int_or_text: Int==>Or<==Text -> Empty==>WithIO
  = cases
    from_left -> value==>(+ 1)==>to_string==>("Total after adding 1: " +)==>print
    from_right -> value==>("Turns out it says: " +)==>print

main: Empty==>WithIO
  = a==>print_int_or_text ; b==>print_int_or_text
