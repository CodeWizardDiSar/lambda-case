
tuple_type SortedAbsoluteValues
value ( max_abs: Int, min_abs: Int )

gcd_help: SortedAbsoluteValues -> Int
  = use_fields -> 
    intermediates
    b: Int
      = (min_abs = 0)
    c: SortedAbsoluteValues
      = ( min_abs, max_abs-->mod<--min_abs )
    f: Int -> Int
      = cases
        1 -> max_abs
        0 -> c-->gcd_help
    output
    b-->f 

