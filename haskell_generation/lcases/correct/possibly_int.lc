
or_type PossiblyInt
values int_wrapper<==(value: Int) | not_int

plus1: PossiblyInt -> PossiblyInt
  = cases
    int_wrapper -> int_wrapper<==(value + 1)
    not_int -> not_int

any_func: (Int -> Int, PossiblyInt) -> PossiblyInt
  = f -> cases
    int_wrapper -> value==>f==>int_wrapper
    not_int -> not_int
